@model List<Transaction>
@{
    ViewBag.Title = "GetTransaction";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h4>Kitap Adı: @ViewBag.UserBook</h4>
<br />
<table class="table table-danger">
    <tr>
        <th>Kullanıcı</th>
        <th>Alış Tarihi</th>
        <th>İade Tarihi</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@(x.UserNoNavigation.FirstName) @(x.UserNoNavigation.LastName)</td>
            <td>@Convert.ToDateTime(x.PurchaseDate).ToString("dd/MM/yyyy")</td>
            <td>@Convert.ToDateTime(x.UserReturnBook).ToString("dd/MM/yyyy")</td>
        </tr>
    }
</table>

